    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "prop-types": "^15.7.2",
    "react": ">=18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-bootstrap-typeahead": "^6.3.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": ">=18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
